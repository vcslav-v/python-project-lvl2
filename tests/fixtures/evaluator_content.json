{
    "test_stylish": 
    {
        "args": 
        [
            {"node": "root",
                "leafs":
                    {"key": "group1", "value": 888, "diff": "add"},
                "children": [{"node": "common",
                    "leafs": [{"key": "setting1",
                        "value": "Value 1",
                        "diff": "no change"},
                    {"key": "setting2", "value": 200, "diff": "no change"},
                    {"key": "setting3", "value": true, "diff": "remove"},
                    {"key": "setting3", "value": false, "diff": "add"}],
                    "children": [{"node": "setting6",
                        "leafs": [{"key": "key",
                        "value": "value",
                        "diff": "no change"}],
                        "children": [{"node": "doge",
                        "leafs": [{"key": "wow",
                            "value": "",
                            "diff": "remove"},
                            {"key": "wow1",
                                "value": "",
                                "diff": "add"}],
                        "children": [],
                        "path": ["root", "common", "setting6"], "diff": "no change"}],
                        "path": ["root", "common"], "diff": "no change"}],
                    "path": ["root"], "diff": "no change"},
                    {"node": "group1",
                        "leafs": [{"key": "test", "value": "test", "diff": "remove"}],
                        "children": [],
                        "path": ["root"], "diff": "remove"}
                    ],
                "path": [], "diff": "no change"}
        ],
        "expect":
            [
                "+ group1: 888",
                "common: {",
                "setting1: Value 1",
                "setting2: 200",
                "- setting3: True",
                "+ setting3: False",
                "setting6: {",
                "key: value",
                "doge: {",
                "- wow:",
                "+ wow1:",
                "}",
                "}",
                "}",
                "+ group1: {",
                "test: test",
                "}"
            ]
    },
    "test_stylish_empty_diff":
    {
        "args":
        [
            {"node": "root", "leafs": [], "children": [], "path": [], "diff": "no change"}
        ],
        "expect": "{\n}"
    },
    "test_stylish_wrong_attr":
    {
        "args":
        [
            {"node": "root", "leafs": 0, "children": [], "path": []},
            {"node": "rot", "leafs": [], "children": [], "path": []}
        ]
    },
    "test_generate_diff_flat_json":
    {
        "expect": {
            "node":"root", "leafs": [
                {"key": "host", "value": "hexlet.io", "diff": "no change"},
                {"key": "timeout", "value": 50, "diff": "remove"},
                {"key": "timeout", "value": 20, "diff": "add"},
                {"key": "proxy", "value": "123.234.53.22", "diff": "remove"},
                {"key": "follow", "value": false, "diff": "remove"},
                {"key": "verbose", "value": true, "diff": "add"}
            ], "children": [], "path": [], "diff": "no change"}
    },
    "test_generate_diff_flat_yaml":
    {
        "expect": {
            "node":"root", "leafs": [
                {"key": "test", "value": "some_string", "diff": "remove"},
                {"key": "num", "value": 43, "diff": "remove"},
                {"key": "ip_address", "value": "192.168.1.1", "diff": "remove"},
                {"key": "ip_address", "value": "255.255.1.1", "diff": "add"},
                {"key": "boolean", "value": false, "diff": "no change"},
                {"key": "host", "value": "hexlet.io", "diff": "add"}
            ], "children": [], "path": [], "diff": "no change"}
    },
    "test_generate_diff_tree_json":
    {
        "expect": {
            "node":"root", "leafs": [
                {"key": "key", "value": "value", "diff": "remove"}
            ], "children": [
                {"node":"common", "leafs": [
                    {"key": "setting1", "value": "Value 1", "diff": "no change"},
                    {"key": "setting2", "value": 200, "diff": "remove"},
                    {"key": "setting3", "value": true, "diff": "remove"},
                    {"key": "setting3", "value": null, "diff": "add"},
                    {"key": "setting4", "value": "blah blah", "diff": "add"},
                    {"key": "follow", "value": false, "diff": "add"}
                ], "children": [
                    {"node": "setting6", "leafs":[
                        {"key": "key", "value": "value", "diff": "no change"},
                        {"key": "ops", "value": "vops", "diff": "add"}
                    ], "children":[
                        {"node":"doge", "leafs": [
                            {"key": "wow", "value": "", "diff": "remove"},
                            {"key": "wow", "value": "so much", "diff": "add"}
                        ], "children": [], "path": ["root", "common", "setting6"], "diff": "no change"}
                    ], "path": ["root", "common"], "diff": "no change"},
                    {"node": "setting5", "leafs":[
                        {"key": "key5", "value": "value5", "diff": "add"}
                    ], "children":[], "path": ["root","common"], "diff": "add"}
                ], "path": ["root"], "diff": "no change"},
                {"node":"key", "leafs": [
                    {"key": "foo", "value": "bar", "diff": "add"}
                ], "children": [], "path": ["root"], "diff": "add"}
            ], "path": [], "diff": "no change"}
    },
