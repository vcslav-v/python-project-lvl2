                          "value": "value5",
                          "path": ["root", "common", "setting5"]}],
                        "children": [],
                        "path": ["root", "common"]},
                       {"node": "setting6",
                        "leafs": [{"key": "key",
                          "value": "value",
                          "path": ["root", "common", "setting6"]},
                         {"key": "ops", "value": "vops", "path": ["root", "common", "setting6"]}],
