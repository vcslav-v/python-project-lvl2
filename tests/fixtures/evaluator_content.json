{
    "test_stylish": 
    {
        "args": 
        [
            {"node": "root",
                "leafs": [
                    {"key": "group1", "value": 888, "parrent": ["root"], "diff": "add"}
                ],
                "children": [{"node": "common",
                    "leafs": [{"key": "setting1",
                        "value": "Value 1",
                        "parrent": ["root", "common"], "diff": "no change"},
                    {"key": "setting2", "value": 200, "parrent": ["root", "common"], "diff": "no change"},
                    {"key": "setting3", "value": true, "parrent": ["root", "common"], "diff": "remove"},
                    {"key": "setting3", "value": false, "parrent": ["root", "common"], "diff": "add"}],
                    "children": [{"node": "setting6",
                        "leafs": [{"key": "key",
                        "value": "value",
                        "parrent": ["root", "common", "setting6"], "diff": "no change"}],
                        "children": [{"node": "doge",
                        "leafs": [{"key": "wow",
                            "value": "",
                            "parrent": ["root", "common", "setting6", "doge"], "diff": "remove"},
                            {"key": "wow1",
                                "value": "",
                                "parrent": ["root", "common", "setting6", "doge"], "diff": "add"}],
                        "children": [],
                        "path": ["root", "common", "setting6"], "diff": "no change"}],
                        "path": ["root", "common"], "diff": "no change"}],
                    "path": ["root"], "diff": "no change"},
                    {"node": "group1",
                        "leafs": [{"key": "test", "value": "test", "parrent": ["root", "group1"]}],
                        "children": [],
                        "path": ["root"], "diff": "remove"}
                    ],
                "path": [], "parrent": [], "diff": "no change"}
        ],
        "expect":
            [
                "+ group1: 888",
                "common: {",
                "setting1: Value 1",
                "setting2: 200",
                "- setting3: True",
                "+ setting3: False",
                "setting6: {",
                "key: value",
                "doge: {",
                "- wow:",
                "+ wow1:",
                "}",
                "}",
                "}",
                "+ group1: {",
                "test: test"
            ]
    },
    "test_stylish_empty_diff":
    {
        "args":
        [
            {"node": "root", "leafs": [], "children": [], "path": [], "diff": "no change"}
        ],
        "expect": "{\n}"
    },
    "test_stylish_wrong_attr":
    {
        "args":
        [
            {"node": "root", "leafs": 0, "children": [], "path": []},
            {"node": "rot", "leafs": [], "children": [], "path": []}
        ]
    },
