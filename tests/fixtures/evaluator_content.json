    "test_get_diff":
    {
        "args":[
            {"node": "root",
                "leafs": {"group1": "test"},
                "children": [{"node": "common",
                  "leafs": {"setting1":"Value 1",
                   "setting2": 200,
                   "setting3": true},
                  "children": [{"node": "setting6",
                    "leafs": {"key": "value"},
                    "children": [{"node": "doge",
                      "leafs": {"wow": ""},
                      "children": [],
                      "path": ["root", "common", "setting6"]}],
                    "path": ["root", "common"]}],
                  "path": ["root"]}],
                "path": []},
                {"node": "root",
                    "leafs": [],
                    "children": [{"node": "common",
                      "leafs": {"follow":false,
                       "setting1":"Value 1",
                       "setting3":null,
                       "setting4":"blah blah"},
                      "children": [{"node": "setting5",
                        "leafs": {"key5": "value5"},
                        "children": [],
                        "path": ["root", "common"]},
                       {"node": "setting6",
                        "leafs": {"key": "value",
                         "ops": "vops"},
                        "children": [{"node": "doge",
                          "leafs": {"wow": "so much"},
                          "children": [],
                          "path": ["root", "common", "setting6"]}],
                        "path": ["root", "common"]}],
                      "path": ["root"]},
                     {"node": "key",
                      "leafs": {"foo": "bar"},
                      "children": [],
                      "path": ["root"]}],
                    "path": []}
        ],
        "expect":{
            "node":"root", "leafs": [
                {"key": "key", "value": "value", "diff": "remove"}
            ], "children": [
                {"node":"common", "leafs": [
                    {"key": "setting1", "value": "Value 1", "diff": "no change"},
                    {"key": "setting2", "value": 200, "diff": "remove"},
                    {"key": "setting3", "value": true, "diff": "remove"},
                    {"key": "setting3", "value": null, "diff": "add"},
                    {"key": "setting4", "value": "blah blah", "diff": "add"},
                    {"key": "follow", "value": false, "diff": "add"}
                ], "children": [
                    {"node": "setting6", "leafs":[
                        {"key": "key", "value": "value", "diff": "no change"},
                        {"key": "ops", "value": "vops", "diff": "add"}
                    ], "children":[
                        {"node":"doge", "leafs": [
                            {"key": "wow", "value": "", "diff": "remove"},
                            {"key": "wow", "value": "so much", "diff": "add"}
                        ], "children": [], "path": ["root", "common", "setting6"], "diff": "no change"}
                    ], "path": ["root", "common"], "diff": "no change"},
                    {"node": "setting5", "leafs":[
                        {"key": "key5", "value": "value5", "diff": "add"}
                    ], "children":[], "path": ["root","common"], "diff": "add"}
                ], "path": ["root"], "diff": "no change"},
                {"node":"key", "leafs": [
                    {"key": "foo", "value": "bar", "diff": "add"}
                ], "children": [], "path": ["root"], "diff": "add"}
            ], "path": [], "diff": "no change"}
    },
