    "test_generate_diff_stylish":
    {
        "expect":[
            "{",
            "- key: value",
            "common: {",
            "setting1: Value 1",
            "- setting2: 200",
            "- setting3: true",
            "+ setting3: null",
            "+ follow: false",
            "+ setting4: blah blah",
            "setting6: {",
            "key: value",
            "+ ops: vops",
            "doge: {",
            "- wow:",
            "+ wow: so much",
            "}",
            "}",
            "+ setting5: {",
            "key5: value5",
            "}",
            "}",
            "+ key: {",
            "foo: bar",
            "}",
            "}"
        ]
    },
