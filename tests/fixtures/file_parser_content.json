{
    "test_get_extension_simple_file": 
    {
        "args": ["file_name.json"],
        "expect": "json"
    },
    "test_get_extension_unix_file_path":
    {
        "args": ["/Users/user/Downloads/file_name.yaml"],
        "expect": "yaml"
    },
    "test_get_extension_win_file_path":
    {
        "args": ["C:\\\\Downloads\\file_name.png"],
        "expect": "png"
    },
    "test_get_extension_many_dots":
    {
        "args": ["file.name.exe"],
        "expect": "exe"
    },
    "test_get_data_json":
    {
        "expect":
        {
            "host": "hexlet.io",
            "timeout": 50,
            "proxy": "123.234.53.22",
            "follow": false
        }
    },
    "test_get_data_yaml":
    {
        "expect":
        {
            "test": "some_string",
            "num": 43,
            "ip_address": "192.168.1.1",
            "boolean": false
        }
    },
    "test_get_repr":
    {
        "args": [
            {
                "common": {
                  "setting1": "Value 1",
                  "setting2": 200,
                  "setting3": true,
                  "setting6": {
                    "key": "value",
                    "doge": {
                      "wow": ""
                    }
                  }
                },
                "group1": {
                  "baz": "bas",
                  "foo": "bar",
                  "nest": {
                    "key": "value"
                  }
                },
                "group2": {
                  "abc": 12345,
                  "deep": {
                    "id": 45
                  }
                }
              }
        ],
        "expect":
            {"node": null, "leafs": null, "children":[
                {"node":"common", "leafs": [
                    {"key":"setting1", "value": "Value 1"},
                    {"key":"setting2", "value": 200},
                    {"key":"setting3", "value": true}
                ], "children":[
                    {"node":"setting6", "leafs": [
                        {"key":"key", "value": "value"}
                    ], "children":[
                        {"node":"doge", "leafs": [
                            {"key":"wow", "value": ""}
                        ], "children": null}
                    ]}
                ]},
                {"node":"group1", "leafs": [
                    {"key":"baz", "value": "bas"},
                    {"key":"foo", "value": "bar"}
                ], "children":[
                    {"node":"nest", "leafs": [
                        {"key":"key", "value": "value"}
                    ], "children":null}
                ]},
                {"node":"group2", "leafs":[
                    {"key":"abc", "value": 12345}
                ], "children":[
                    {"node":"deep", "leafs": [
                        {"key":"id", "value": 45}
                    ]}
                ]}     
            ]}
    }
}
