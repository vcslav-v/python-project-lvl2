{
    "test_get_extension_simple_file": 
    {
        "args": ["file_name.json"],
        "expect": "json"
    },
    "test_get_extension_unix_file_path":
    {
        "args": ["/Users/user/Downloads/file_name.yaml"],
        "expect": "yaml"
    },
    "test_get_extension_win_file_path":
    {
        "args": ["C:\\\\Downloads\\file_name.png"],
        "expect": "png"
    },
    "test_get_extension_many_dots":
    {
        "args": ["file.name.exe"],
        "expect": "exe"
    },
    "test_get_data_json":
    {
        "expect":
        {
            "host": "hexlet.io",
            "timeout": 50,
            "proxy": "123.234.53.22",
            "follow": false
        }
    },
    "test_get_data_yaml":
    {
        "expect":
        {
            "test": "some_string",
            "num": 43,
            "ip_address": "192.168.1.1",
            "boolean": false
        }
    },
    "test_get_repr":
    {
        "args": [
            {
                "common": {
                  "setting1": "Value 1",
                  "setting2": 200,
                  "setting3": true,
                  "setting6": {
                    "key": "value",
                    "doge": {
                      "wow": ""
                    }
                  }
                },
                "group1": "test"
            }
        ],
        "expect":
        {"node": "root",
          "leafs": [{"key": "group1", "value": "test", "path": ["root"]}],
          "children": [{"node": "common",
            "leafs": [{"key": "setting1",
              "value": "Value 1",
              "path": ["root", "common"]},
             {"key": "setting2", "value": 200, "path": ["root", "common"]},
             {"key": "setting3", "value": true, "path": ["root", "common"]}],
            "children": [{"node": "setting6",
              "leafs": [{"key": "key",
                "value": "value",
                "path": ["root", "common", "setting6"]}],
              "children": [{"node": "doge",
                "leafs": [{"key": "wow",
                  "value": "",
                  "path": ["root", "common", "setting6", "doge"]}],
                "children": [],
                "path": ["root", "common", "setting6"]}],
              "path": ["root", "common"]}],
            "path": ["root"]}],
          "path": []}
    }
}
